            <row><entry namest="col-key" nameend="col-desc" valign="top">ACL Management APIs</entry></row>
          </thead>
          <tbody>
            <row>
              <entry>
                <codeph>
                  <ph conref="post-assetapi-acl-projpath.dita#post-assetapi-acl-projpath/ph-method"/>
                  <ph conref="post-assetapi-acl-projpath.dita#post-assetapi-acl-projpath/ph-sig"/>
                </codeph>
